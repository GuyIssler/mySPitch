<%= bootstrap_form_for(@candidate_feedback) do |f| %>

    <%= f.alert_message t('alerts.model_generic') %>


    <%= f.text_field :rating, class: "input-large" %>
    <%= f.text_field :strengths,class: "input-large" %>

    <%= f.text_field :weaknesses, class: "input-large" %>

    <%= f.text_area :feedback,  class: "input-xxlarge" %>

    <%= f.hidden_field :user_id, value:@candidate.id %>

    <%= f.actions do %>
        <%= f.primary t('buttons.submit'), disable_with: t('buttons.saving') %>
    <% end %>
<% end %>



